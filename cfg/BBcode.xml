<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE list SYSTEM "../system/DTD/BBcode.dtd">
<list>
    <BBcode tag="b" closeTag="true" html="&lt;b&gt;{text}&lt;/b&gt;"/>
    <BBcode tag="u" closeTag="true" html="&lt;u&gt;{text}&lt;/u&gt;"/>
    <BBcode tag="i" closeTag="true" html="&lt;i&gt;{text}&lt;/i&gt;"/>
    <BBcode tag="s" closeTag="true" html="&lt;span style=&quot;text-decoration: line-through;&quot; &gt;{text}&lt;/span&gt;"/>
    <BBcode tag="url" closeTag="true" html="&lt;a href=&quot;{param}&quot; &gt;{text}&lt;/a&gt;" param="true"/>
    <BBcode tag="url" closeTag="true" html="&lt;a href=&quot;{text}&quot; &gt;{text}&lt;/a&gt;" param="false"/>
    <BBcode tag="img" closeTag="true" html="&lt;img src=&quot;{text}&quot; alt=&quot;image&quot; /&gt;"/>
    <BBcode tag="img" closeTag="false" html="&lt;img src=&quot;{param}&quot; alt=&quot;image&quot; /&gt;" param="true"/>
    <BBcode tag="img" closeTag="true" html="&lt;img src=&quot;{url}&quot; alt=&quot;{text}&quot; /&gt;" params="url"/>
    <BBcode tag="img" closeTag="true" html="&lt;img src=&quot;{url}&quot; alt=&quot;{text}&quot; class=&quot;align-{align}&quot;/&gt;" params="url, align"/>
    <BBcode tag="img" closeTag="false" html="&lt;img src=&quot;{url}&quot; class=&quot;align-{align}&quot;/&gt;" params="url, align"/>
    <BBcode tag="img" closeTag="true" html="&lt;img src=&quot;{url}&quot; alt=&quot;{text}&quot; class=&quot;align-{align}&quot; style=&quot;width: {width};&quot;/&gt;" params="url, align, width"/>
    <BBcode tag="img" closeTag="false" html="&lt;img src=&quot;{url}&quot; class=&quot;align-{align}&quot; style=&quot;width: {width}&quot;/&gt;" params="url, align, width"/>
    <BBcode tag="img" closeTag="true" html="&lt;img src=&quot;{param}&quot; alt=&quot;{text}&quot; /&gt;" param="true"/>
    <BBcode tag="font" closeTag="true" html="&lt;span style=&quot;font-family: {param}&quot;&gt;{text}&lt;/span&gt;" param="true"/>
    <BBcode tag="center" closeTag="true" html="&lt;div class=&quot;centered&quot;&gt;{text}&lt;/div&gt;"/>
    <BBcode tag="list" closeTag="true" html="&lt;ul&gt;{text}&lt;/ul&gt;"/>
    <BBcode tag="numlist" closeTag="true" html="&lt;ol&gt;{text}&lt;/ol&gt;"/>
    <BBcode tag="\*" closeTag="true" html="&lt;li&gt;{text}&lt;/li&gt;"/>
    <BBcode tag="changelog" closeTag="true" html="&lt;ul class=&quot;changelog&quot;&gt;{text}&lt;/ul&gt;"/>
    <BBcode tag="\+" closeTag="true" html="&lt;li class=&quot;added&quot;&gt;{text}&lt;/li&gt;"/>
    <BBcode tag="-" closeTag="true" html="&lt;li class=&quot;removed&quot;&gt;{text}&lt;/li&gt;"/>
    <BBcode tag="color" closeTag="true" html="&lt;span style=&quot;color: {param};&quot; &gt;{text}&lt;/span&gt;" param="true"/>
    <BBcode tag="size" closeTag="true" html="&lt;span style=&quot;font-size: {param};&quot; &gt;{text}&lt;/span&gt;" param="true"/>
    <BBcode tag="hr" closeTag="false" html="&lt;hr /&gt;"/>
    <BBcode tag="acr" closeTag="true" html="&lt;abbr title=&quot;{param}&quot;&gt;{text}&lt;/abbr&gt;" param="true"/>
    <BBcode tag="break" closeTag="false" html="&lt;hr class=&quot;break&quot; /&gt;"/>
    <BBcode tag="more" closeTag="false" html="&lt;hr class=&quot;break&quot; /&gt;"/>
    <BBcode tag="bbcode" closeTag="true" html="&lt;pre class=&quot;bbcode&quot;&gt;{text}&lt;/pre&gt;"/>
    <BBcode tag="h1" closeTag="true" html="&lt;h1&gt;{text}&lt;/h1&gt;"/>
    <BBcode tag="h2" closeTag="true" html="&lt;h2&gt;{text}&lt;/h2&gt;"/>
    <BBcode tag="h3" closeTag="true" html="&lt;h3&gt;{text}&lt;/h3&gt;"/>
    <BBcode tag="h4" closeTag="true" html="&lt;h4&gt;{text}&lt;/h4&gt;"/>
    <BBcode tag="h5" closeTag="true" html="&lt;h5&gt;{text}&lt;/h5&gt;"/>
    <BBcode tag="h6" closeTag="true" html="&lt;h6&gt;{text}&lt;/h6&gt;"/>
    <BBcode tag="quote" closeTag="true" html="&lt;blockquote&gt;{text}&lt;div class=&quot;author&quot;&gt;{param}&lt;/div&gt;&lt;/blockquote&gt;" param="true"/>
    <BBcode tag="quote" closeTag="true" html="&lt;blockquote&gt;{text}&lt;/blockquote&gt;"/>
    <BBcode tag="align" closeTag="true" html="&lt;span style=&quot;float: {param};&quot;&gt;{text}&lt;/span&gt;" param="true"/>
    <BBcode tag="table" closeTag="true" html="&lt;table class=&quot;data-table&quot;&gt;{text}&lt;/table&gt;"/>
    <BBcode tag="table" closeTag="true" html="&lt;table class=&quot;data-table&quot; style=&quot;width: {param}&quot;&gt;{text}&lt;/table&gt;" param="true"/>
    <BBcode tag="table" closeTag="true" html="&lt;table class=&quot;data-table align-{align}&quot; style=&quot;width: {width}&quot;&gt;{text}&lt;/table&gt;" params="align, width"/>
    <BBcode tag="row" closeTag="true" html="&lt;tr&gt;{text}&lt;/tr&gt;"/>
    <BBcode tag="cell" closeTag="true" html="&lt;td&gt;{text}&lt;/td&gt;"/>
    <BBcode tag="header" closeTag="true" html="&lt;th&gt;{text}&lt;/th&gt;"/>
    <BBcode tag="header" closeTag="true" html="&lt;th width=&quot;{param}&quot;&gt;{text}&lt;/th&gt;" param="true"/>
    <BBcode tag="sub" closeTag="true" html="&lt;sub&gt;{text}&lt;/sub&gt;"/>
    <BBcode tag="sup" closeTag="true" html="&lt;sup&gt;{text}&lt;/sup&gt;"/>
    <BBcode tag="clear" closeTag="false" html="&lt;hr style=&quot;visibility: hidden; clear: both; margin: 0; height: 0; padding 0;&quot;/&gt;"/>
</list>
